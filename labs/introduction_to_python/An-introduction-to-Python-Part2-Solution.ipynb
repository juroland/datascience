   "source": [
    "def fibo(n):\n",
    "    n0 = 1\n",
    "    n1 = 1\n",
    "    for i in range(n):\n",
    "        yield n0\n",
    "        n0, n1 = n1, n0 + n1\n",
    "        \n",
    "for f in fibo(20):\n",
    "    print(f)"
   ]
  }
